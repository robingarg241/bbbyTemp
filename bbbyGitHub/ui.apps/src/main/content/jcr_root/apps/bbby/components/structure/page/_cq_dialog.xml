<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <seo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/bbby/components/structure/page/tab-seo">
                    </seo>
                    <misc
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/bbby/components/structure/page/tab-misc">
                    </misc>

                    <socialmedia jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <section jcr:primaryType="nt:unstructured">
                                        <items jcr:primaryType="nt:unstructured">
                                            <field jcr:primaryType="nt:unstructured">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <twitter
                                                        cq:showOnCreate="{Boolean}true"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        name="./socialMedia"
                                                        renderReadOnly="{Boolean}true"
                                                        text="Twitter"
                                                        value="twitter">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-msm-lockable="socialMedia"/>
                                                    </twitter>
                                                </items>
                                            </field>
                                        </items>
                                    </section>
                                </items>
                            </column>
                        </items>
                    </socialmedia>
                    <uploadtype
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        jcr:title="Asset Upload">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <uploadtype
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Upload Type"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/select"
                                                fieldLabel="Excel Metadata Asset Uploader"
                                                name="./uploadType"
                                                renderReadOnly="true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <single
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Single"
                                                        value="single"/>
                                                    <multi
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Multi"
                                                        value="multi"/>
                                                    <fasttrack
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Fast-Track"
                                                        value="fasttrack"/>
                                                </items>
                                            </type>
                                        </items>
                                    </uploadtype>
                                </items>
                            </column>
                        </items>
                    </uploadtype>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
