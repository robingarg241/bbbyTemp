<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:rep="internal"
    cq:showOnCreate="{Boolean}false"
    jcr:primaryType="nt:unstructured"
    jcr:title="Miscellaneous"
    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
    <items jcr:primaryType="nt:unstructured">
        <column
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
            <items jcr:primaryType="nt:unstructured">
                <settings
                    cq:showOnCreate="{Boolean}true"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Settings"
                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                    <items jcr:primaryType="nt:unstructured">
                        <errorPages
                            cq:showOnCreate="{Boolean}true"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                            cq-msm-lockable="./errorPages"
                            fieldDescription="The root page containing the error pages for this site."
                            fieldLabel="Error Pages"
                            name="./errorPages"
                            renderReadOnly="true"
                            required="{Boolean}false"
                            rootPath="/content/bbby-com/"/>
                    </items>
                </settings>
                <visibility
                    cq:showOnCreate="{Boolean}true"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Visibility"
                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                    <items jcr:primaryType="nt:unstructured">
                        <hideinbreadcrumbs
                            jcr:primaryType="nt:unstructured"
                            sling:orderBefore="hideInNav"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            allowBulkEdit="{Boolean}true"
                            cq-msm-lockable="hideInBreadcrumbs"
                            name="./hideInBreadcrumbs"
                            renderReadOnly="{Boolean}true"
                            text="Hide in Breadcrumbs"
                            value="true"/>
                        <hideinaemsearch
                            jcr:primaryType="nt:unstructured"
                            sling:orderBefore="hideInNav"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            allowBulkEdit="{Boolean}true"
                            cq-msm-lockable="hideInAemSearch"
                            name="./hideInAemSearch"
                            renderReadOnly="{Boolean}true"
                            text="Hide in AEM Search"
                            value="true"/>
                    </items>
                </visibility>
            </items>
        </column>
    </items>
</jcr:root>
